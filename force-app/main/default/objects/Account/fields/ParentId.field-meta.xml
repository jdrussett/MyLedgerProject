<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ParentId</fullName>
    <lookupFilter>
        <active>true</active>
        <booleanFilter>(1 AND 2) OR 3</booleanFilter>
        <errorMessage>You must select a checking account to act as the parent account for a card account from which the card will be paid down.</errorMessage>
        <filterItems>
            <field>Account.Type</field>
            <operation>equals</operation>
            <value>Checking Account</value>
        </filterItems>
        <filterItems>
            <field>$Source.Type</field>
            <operation>equals</operation>
            <value>Credit Card</value>
        </filterItems>
        <filterItems>
            <field>$Source.Type</field>
            <operation>notEqual</operation>
            <value>Credit Card</value>
        </filterItems>
        <infoMessage>Select the checking account that will pay off the card account.</infoMessage>
        <isOptional>false</isOptional>
    </lookupFilter>
    <trackFeedHistory>true</trackFeedHistory>
    <type>Hierarchy</type>
</CustomField>
